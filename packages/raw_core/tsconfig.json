{
  "extends": "@react-raw/tsconfig/base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~context/*": ["src/context/*"],
      "~hook/*": ["src/hook/*"],
      "~raw/*": ["src/raw/*"]
    },
    "jsx": "react-jsx",
    "tsBuildInfoFile": "./dist/.tsbuildinfo"
  },
  "include": ["**/*.ts", "**/*.tsx", "package.json"],
  "exclude": ["node_modules", "dist"],
  "references": [
    {
      "path": "../raw_bundler"
    },
    {
      "path": "../raw_lib"
    }
  ]
}
