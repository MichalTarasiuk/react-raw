{
  "extends": "@react-raw/tsconfig/base.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./dist/.tsbuildinfo",
    "baseUrl": ".",
    "jsx": "react-jsx",
    "paths": {
      "~components/*": ["src/components/*"],
      "~composables/*": ["src/composables/*"],
      "~node/*": ["src/node/*"],
      "~utils/*": ["src/utils/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "package.json", "tsconfig.json"],
  "exclude": ["node_modules", "dist"],
  "references": [
    {
      "path": "../raw_bundler"
    }
  ]
}
